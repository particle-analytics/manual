{
  "title": "Manual",
  "version": "2.1.x",
  "gitbook": ">=2.4.0",
  "language": "en",
  "theme": "theme",
  "variables": {
      "host": "mybook.com"
  },
  "links": {
    "sharing": {
      "google": false,
      "facebook": false,
      "twitter": false,
      "weibo": false,
      "all": false
    }
  },
  "plugins": ["mathjax","collapsible-menu"],
  "pluginsConfig": {
    "mathjax":{
        "forceSVG": true
    },
    "fontSettings": {
        "theme": "white",
        "size": 2
    }
  }
}